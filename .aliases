alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."
alias ~="cd ~"

alias ll="ls -l"
alias la="ls -al"
alias lh="ls -alh"
alias ld='ll | grep "^d"'

alias flush="dscacheutil -flushcache"

alias sl="/Applications/Sublime\ Text.app/Contents/SharedSupport/bin/subl"
alias hosts="sl /etc/hosts"

alias nx="sudo nginx"
alias nxc="sl /usr/local/nginx/conf/nginx.conf"
alias nxr="nx -s reload"
alias svn+="svn add"

alias q="cd ~/workshop/qzz.com"
alias p="cd ~/workshop"
alias flt="cd ~/workshop/fekit/flight_inter"
alias tts="cd ~/workshop/fekit/flight_intertts"
alias fk="cd ~/workshop/fekit"

alias qps="qzz pack && qzz sync && rm -rf dev"
alias fps="fekit pack && fekit sync && rm -rf dev && rm -rf ver && rm -rf refs"
alias gph="git push origin"
alias gpl="git pull origin"
alias gci="git commit -a -m"
alias gst="git st"
alias gsts="git sts"
