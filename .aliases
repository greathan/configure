alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."
alias ~="cd ~"

alias ll="ls -l"
alias la="ls -al"
alias lh="ls -alh"
alias ld='ll | grep "^d"'

alias flush="dscacheutil -flushcache"

alias sl="/Applications/Sublime\ Text.app/Contents/SharedSupport/bin/subl"
alias hosts="sl /etc/hosts"

alias nx="sudo nginx"
alias nxc="sl /usr/local/nginx/conf/nginx.conf"
alias nxr="nx -s reoload"
alias svn+="svn add"

alias q="cd ~/workshop/qzz.com"
alias p="cd ~/workshop"
alias flt="cd ~/workshop/qzz.com/flight"
alias tts="cd ~/workshop/qzz.com/flight_intertts"

alias qps="qzz pack && qzz sync && rm -rf dev"
alias qs="node /usr/local/Qzz/q/lib/qserver.js -p 8000"
